{
  "manifest_version": 3,
  "name": "Productivity Monitor",
  "version": "1.0",
  "description": "Monitors tab activity to help understand workflow.",
  "permissions": [
    "tabs",
    "storage"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "action": {
    "default_popup": "index.html"
    // default_icon removed
  }
  // icons field removed
}
